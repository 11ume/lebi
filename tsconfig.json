{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "removeComments": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "wezi": [
        "packages/wezi"
      ],
      "wezi-composer": [
        "packages/composer"
      ],
      "wezi-cors": [
        "packages/cors"
      ],
      "wezi-error": [
        "packages/error"
      ],
      "wezi-send": [
        "packages/send"
      ],
      "wezi-receive": [
        "packages/receive"
      ],
      "wezi-router": [
        "packages/router"
      ],
      "wezi-types": [
        "packages/types"
      ]
    }
  }
}